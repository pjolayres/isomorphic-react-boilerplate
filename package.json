{
  "name": "isomorphic-react-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "engines": {
    "node": ">=8.14.0"
  },
  "scripts": {
    "start": "node server.js",
    "debug": "shx --silent rm -rf dist && webpack --watch --config webpack.dev.config.babel.js --env.debug",
    "dev": "shx --silent rm -rf dist && cross-env LOG_LEVEL=debug webpack --watch --config webpack.dev.config.babel.js --env.dev",
    "prod": "shx --silent rm -rf dist && cross-env NODE_ENV=production webpack --config webpack.prod.config.babel.js",
    "server": "npm run prod && cross-env NODE_ENV=production node server.js",
    "test": "jest .*\\.test\\.js",
    "test:integration": "jest .*\\.test\\.integration\\.js",
    "lint": "node_modules/.bin/eslint ./src --fix",
    "heroku-postbuild": "npm run prod"
  },
  "author": "",
  "license": "ISC",
